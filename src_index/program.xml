<program>
	<elements>
		<timer id="T1" delay="100" unit="ms" />
		<reg id="R1" />
		<contact id="X1" normal="true" />
		<contact id="X2" normal="false" />
		<coil id="Y1" type="set" />
		<counter id="C1" countTo="10" target="false" />
		<counter id="C2" countTo="20" target="true" />
	</elements>
	<diagram>
		<output id="R1">
			<not>
				<and>
					<and>
						<elem id="X2" />
						<elem id="C1" />
					</and>
					<elem id="X1" />
					<elem id="T1" />
					<not>
						<or>
							<elem id="X1" />
							<not>
								<elem id="C1" />
							</not>
						</or>
					</not>
				</and>
			</not>
		</output>
		<output id="T1">
			<not>
				<elem id="C1" />
			</not>
		</output>
	</diagram>
</program>

<program>
	<elements>
		<reg id="R0" />
		<contact id="T0" normal="true" />
		<contact id="x3" normal="true" />
		<contact id="x2" normal="true" />
		<coil id="Y0" type="set" />
		<contact id="x1" normal="true" />
		<contact id="x6" normal="true" />
		<contact id="x7" normal="true" />
		<contact id="x4" normal="true" />
		<contact id="x5" normal="true" />
	</elements>
	<diagram>
		<output id="Y0">
			<and>
				<or>
					<elem id="x5" />
					<elem id="x6" />
					<elem id="x7" />
				</or>
				<or>
					<and>
						<elem id="x4" />
						<or>
							<elem id="x1" />
							<elem id="x2" />
						</or>
					</and>
					<elem id="x3" />
				</or>
			</and>
		</output>
		<output id="T0">
			<and>
				<or>
					<elem id="x1" />
					<elem id="x2" />
				</or>
				<elem id="Y0" />
			</and>
		</output>
		<output id="R0">
			<and>
				<elem id="x1" />
				<or>
					<elem id="T0" />
					<elem id="Y0" />
					<elem id="x1" />
				</or>
			</and>
		</output>
	</diagram>
</program>
